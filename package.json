{
  "name": "repo-file-sync-action",
  "version": "1.19.0",
  "type": "module",
  "description": "GitHub Action to keep files like Action workflows or entire directories in sync between multiple repositories.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run package",
    "format": "prettier --write '**/*.js'",
    "format-check": "prettier --check '**/*.js'",
    "lint": "eslint src/**/*.js",
    "package": "ncc build src/index.js --out dist --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "engines": {
    "node": ">= 24"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisCarini/repo-file-sync-action.git"
  },
  "keywords": [
    "sync",
    "file-sync",
    "workflow-sync",
    "github-workflow",
    "github-actions",
    "github-action-workflow-sync"
  ],
  "author": "Maximilian Schiller <schiller@mxis.ch>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChrisCarini/repo-file-sync-action/issues"
  },
  "homepage": "https://github.com/marketplace/actions/repo-file-sync-action",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/github": "^9.0.0",
    "@octokit/plugin-throttling": "^11.0.3",
    "@putout/git-status-porcelain": "^3.0.0",
    "action-input-parser": "^1.2.38",
    "dedent-js": "^1.0.1",
    "fs-extra": "^11.3.3",
    "js-yaml": "^4.1.1",
    "node-readfiles": "0.4.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@betahuhn/config": "^1.2.0",
    "@octokit/core": "^7.0.6",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.11.0",
    "jest": "^30.2.0",
    "prettier": "3.7.4"
  },
  "volta": {
    "node": "24.5.0"
  }
}
